  "scripts": {
    "clean": "node scripts/clean-build.js",
    "clean:cache": "rm -rf node_modules/.cache && jest --clearCache",
    "build": "npm run clean && tsc",
    "build:verify": "npm run build && npm run verify:build",
    "verify:build": "node scripts/verify-build.js",
    "dev": "ts-node src/cli.ts",
    "pretest": "npm run clean",
    "test": "jest",
    "test:clean": "npm run clean && npm run clean:cache && npm test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:audit": "jest src/tests/faf-edge-case-audit.test.ts --verbose",
    "test:audit-watch": "jest src/tests/faf-edge-case-audit.test.ts --watch --verbose",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "emoji:check": "bash .githooks/pre-commit-emoji-check.sh",
    "emoji:audit": "(grep -r 'ðŸŽ¯\\|âœ…' src/ | grep -v 'FORBIDDEN' || echo 'â˜‘ï¸ No forbidden emojis found!')",
    "build:engine": "webpack --config webpack.mk2.config.js",
