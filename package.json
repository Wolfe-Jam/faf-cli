    "build:protected": "npm run build:engine && npm run obfuscate:engine",
    "build:all": "npm run build:protected && npm run build",
    "prepare": "npm run build && npm run test:audit",
    "sync-version": "node scripts/sync-version.js",
    "version": "npm run sync-version && git add project.faf",
    "prepublishOnly": "npm run build:verify && npm run version:truth",
    "docs:generate": "ts-node scripts/generate-docs.ts",
    "docs:build": "cd docs-site && npm run build",
    "docs:dev": "cd docs-site && npm run dev",
    "docs:deploy": "cd docs-site && vercel --prod",
    "docs:preview": "cd docs-site && vercel",
    "release": "ts-node scripts/prepare-release.ts",
    "release:patch": "npm run release -- --type patch",
    "release:minor": "npm run release -- --type minor",
    "release:major": "npm run release -- --type major",
    "release:beta": "npm run release -- --type prerelease --preid beta",
    "release:dry": "npm run release -- --dry-run",
    "performance": "ts-node scripts/performance-validation.ts",
    "performance:ci": "npm run performance -- --tests status_command,help_command,index_command",
    "performance:full": "npm run performance -- --verbose",
    "version:update": "node scripts/update-version.js",
    "version:check": "node scripts/version-check.js",
    "version:truth": "node scripts/version-check.js",
    "prepublish:check": "npm run version:truth",
    "precommit": "npm run version:check",
    "showcase": "ts-node scripts/run-industry-showcase.ts",
    "showcase:build": "npm run build && npm run showcase",
    "showcase:test": "ts-node scripts/run-test-showcase.ts",
    "postinstall": "node scripts/postinstall.js"
